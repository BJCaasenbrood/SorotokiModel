# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/MATLAB/R2023a
startdir = /home/brandon/Documents/projects/SorotokiModel/mex/forwardkin

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=computeForwardKinematicsFast_mex -msse2 -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=computeForwardKinematicsFast_mex -msse2 -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -Wl,-Bsymbolic -o computeForwardKinematicsFast_mex.mexa64 -L"$matlabdir/bin/glnxa64" -lmwblas -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,computeForwardKinematicsFast_mex.map

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/computeForwardKinematicsFast_data.o : cc $root/computeForwardKinematicsFast_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/computeForwardKinematicsFast_initialize.o : cc $root/computeForwardKinematicsFast_initialize.c
build $builddir/computeForwardKinematicsFast_terminate.o : cc $root/computeForwardKinematicsFast_terminate.c
build $builddir/computeForwardKinematicsFast.o : cc $root/computeForwardKinematicsFast.c
build $builddir/mtimes.o : cc $root/mtimes.c
build $builddir/_coder_computeForwardKinematicsFast_api.o : cc $root/interface/_coder_computeForwardKinematicsFast_api.c
build $builddir/_coder_computeForwardKinematicsFast_mex.o : cc $root/interface/_coder_computeForwardKinematicsFast_mex.c
build $builddir/computeForwardKinematicsFast_emxutil.o : cc $root/computeForwardKinematicsFast_emxutil.c
build $builddir/_coder_computeForwardKinematicsFast_info.o : cc $root/interface/_coder_computeForwardKinematicsFast_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/computeForwardKinematicsFast_mex.mexa64 : link $builddir/computeForwardKinematicsFast_data.o $builddir/rt_nonfinite.o $builddir/computeForwardKinematicsFast_initialize.o $builddir/computeForwardKinematicsFast_terminate.o $builddir/computeForwardKinematicsFast.o $builddir/mtimes.o $builddir/_coder_computeForwardKinematicsFast_api.o $builddir/_coder_computeForwardKinematicsFast_mex.o $builddir/computeForwardKinematicsFast_emxutil.o $builddir/_coder_computeForwardKinematicsFast_info.o $builddir/c_mexapi_version.o 
