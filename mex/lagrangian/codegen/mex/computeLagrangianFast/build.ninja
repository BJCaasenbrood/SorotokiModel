# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/MATLAB/R2023a
startdir = /home/brandon/Documents/MATLAB/SorotokiCode/src/model/mex/lagrangian

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=computeLagrangianFast_mex -msse2 -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=computeLagrangianFast_mex -msse2 -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -Wl,-Bsymbolic -o computeLagrangianFast_mex.mexa64 -L"$matlabdir/bin/glnxa64" -lmwblas -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,computeLagrangianFast_mex.map

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/computeLagrangianFast_data.o : cc $root/computeLagrangianFast_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/computeLagrangianFast_initialize.o : cc $root/computeLagrangianFast_initialize.c
build $builddir/computeLagrangianFast_terminate.o : cc $root/computeLagrangianFast_terminate.c
build $builddir/computeLagrangianFast.o : cc $root/computeLagrangianFast.c
build $builddir/eml_mtimes_helper.o : cc $root/eml_mtimes_helper.c
build $builddir/mtimes.o : cc $root/mtimes.c
build $builddir/_coder_computeLagrangianFast_api.o : cc $root/interface/_coder_computeLagrangianFast_api.c
build $builddir/_coder_computeLagrangianFast_mex.o : cc $root/interface/_coder_computeLagrangianFast_mex.c
build $builddir/computeLagrangianFast_emxutil.o : cc $root/computeLagrangianFast_emxutil.c
build $builddir/_coder_computeLagrangianFast_info.o : cc $root/interface/_coder_computeLagrangianFast_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/computeLagrangianFast_mex.mexa64 : link $builddir/computeLagrangianFast_data.o $builddir/rt_nonfinite.o $builddir/computeLagrangianFast_initialize.o $builddir/computeLagrangianFast_terminate.o $builddir/computeLagrangianFast.o $builddir/eml_mtimes_helper.o $builddir/mtimes.o $builddir/_coder_computeLagrangianFast_api.o $builddir/_coder_computeLagrangianFast_mex.o $builddir/computeLagrangianFast_emxutil.o $builddir/_coder_computeLagrangianFast_info.o $builddir/c_mexapi_version.o 
